#: evesrp/templates/request_pay.html:18
#, python-format
msgid "%(name)s at %(timestamp)s"
msgstr "%(name)s a las %(timestamp)s"

#. Button that if clicked, will close the modal window for adding/viewing notes
#. for a user.
#: evesrp/templates/request_pay.html:34
