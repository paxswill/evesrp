"%(group_name)s(%(member_count)d) <small class=\"text-muted text-"
"right\">from %(auth_source)s</small>"
msgstr ""
"%(group_name)s(%(member_count)d) <small class=“text-muted text-"
"right”>from %(auth_source)s</small>"

